dbMemo "SQL" ="SELECT tbl_Locations.Unit_Code, tbl_Locations.Plot_ID, tbl_Locations.Vegetation_"
    "Type, tbl_Events.Start_Date, tbl_LP_Belt_Transect.Transect, IIf([tlu_NCPN_Plants"
    "]![LU_Code] Is Null,[tbl_LP_Shrub]![Species],[tlu_NCPN_Plants]![LU_Code]) AS Spe"
    "cies_Code, tlu_NCPN_Plants.Utah_Species, tbl_LP_Shrub.HC10, tbl_LP_Shrub.HC25, t"
    "bl_LP_Shrub.HC50, tbl_LP_Shrub.HC100, tbl_LP_Shrub.HC2m, tbl_LP_Shrub.HCGT2\015\012"
    "FROM tbl_Locations INNER JOIN (tbl_Events INNER JOIN (tbl_LP_Belt_Transect INNER"
    " JOIN (tbl_LP_Shrub LEFT JOIN tlu_NCPN_Plants ON tbl_LP_Shrub.Species = tlu_NCPN"
    "_Plants.Master_PLANT_Code) ON tbl_LP_Belt_Transect.Transect_ID = tbl_LP_Shrub.Tr"
    "ansect_ID) ON tbl_Events.Event_ID = tbl_LP_Belt_Transect.Event_ID) ON tbl_Locati"
    "ons.Location_ID = tbl_Events.Location_ID\015\012WHERE (\015\012(tbl_LP_Shrub.HC1"
    "0 Is Null) \015\012OR (tbl_LP_Shrub.HC25 Is Null) \015\012OR (tbl_LP_Shrub.HC50 "
    "Is Null) \015\012OR (tbl_LP_Shrub.HC100 Is Null) \015\012OR (tbl_LP_Shrub.HC2m I"
    "s Null) \015\012OR (tbl_LP_Shrub.HCGT2 Is Null) \015\012OR (\015\012(tbl_LP_Shru"
    "b.HC10=0) \015\012AND (tbl_LP_Shrub.HC25=0) \015\012AND (tbl_LP_Shrub.HC50=0) \015"
    "\012AND (tbl_LP_Shrub.HC100=0) \015\012AND (tbl_LP_Shrub.HC2m=0) \015\012AND (tb"
    "l_LP_Shrub.HCGT2=0)\015\012)\015\012)\015\012AND  tbl_Locations.Unit_Code = 'ARC"
    "H'\015\012AND tbl_Events.Start_Date = #9/29/2010#\015\012AND tbl_Locations.Plot_"
    "ID = 1\015\012ORDER BY tbl_Locations.Unit_Code, tbl_Locations.Plot_ID, tbl_Event"
    "s.Start_Date, tbl_LP_Belt_Transect.Transect, IIf([tlu_NCPN_Plants]![LU_Code] Is "
    "Null,[tbl_LP_Shrub]![Species],[tlu_NCPN_Plants]![LU_Code]);\015\012"
