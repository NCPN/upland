dbMemo "SQL" ="SELECT tbl_Locations.Unit_Code, tbl_Locations.Plot_ID, tbl_Locations.Vegetation_"
    "Type, tbl_Events.Start_Date, tbl_LP_Belt_Transect.Transect\015\012FROM (tbl_Loca"
    "tions INNER JOIN (tbl_Events INNER JOIN tbl_LP_Belt_Transect ON tbl_Events.Event"
    "_ID = tbl_LP_Belt_Transect.Event_ID) ON tbl_Locations.Location_ID = tbl_Events.L"
    "ocation_ID) INNER JOIN tbl_LP_Exotic_Freq ON tbl_LP_Belt_Transect.Transect_ID = "
    "tbl_LP_Exotic_Freq.Transect_ID\015\012WHERE (tbl_LP_Exotic_Freq.Species Is Null "
    "\015\012OR (tbl_LP_Exotic_Freq.Species=\"\") \015\012OR (tbl_LP_Exotic_Freq.Spec"
    "ies)=\" \")\015\012AND  tbl_Locations.Unit_Code = 'BLCA'\015\012AND tbl_Events.S"
    "tart_Date = #4/10/2018#\015\012AND tbl_Locations.Plot_ID = 41\015\012ORDER BY tb"
    "l_Locations.Unit_Code, tbl_Locations.Plot_ID, tbl_Events.Start_Date, tbl_LP_Belt"
    "_Transect.Transect;\015\012"
